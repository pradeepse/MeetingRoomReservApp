@model IEnumerable<MeetingRoomReservApp.usp_MeetingRoomFinder_Result>

<h2> Meeting Room Finder Details</h2>
<table class="table table-bordered">
<thead>
	  <tr>
	      <th>Room ID</th>
	      <th>Location ID</th>
	      <th>From Date</th>
	      <th>To Date</th>
	  </tr>
</thead>
<tbody>
	  @foreach (var item in @Model)
	  {
	      <tr>
	          <td>@item.RoomID</td>
	          <td>@item.LocationID</td>
              <td>@item.FromDate</td>
              <td>@item.ToDate</td>
	          <td><a data-value="@item.Status" href="javascript:void(0)" class="btnEdit">@item.Status</a></td>
	      </tr> 
	  }
</tbody>
</table>


 

   